<Window x:Class="FMP_Common.Management.Category.CategoryManagementView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FMP_Common.Management.Category"
        mc:Ignorable="d"
        Title="CategoryManagementView" Height="450" Width="800">
    <Grid Margin="0,93,0,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="175*"/>
            <ColumnDefinition Width="225*"/>
            <ColumnDefinition Width="204*"/>
        </Grid.ColumnDefinitions>
        <ListBox x:Name="CategoryListBox" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <ListBox x:Name="ModuleOptionListBox" Grid.Column="1" d:ItemsSource="{d:SampleData ItemCount=5}"/>

    </Grid>
</Window>
