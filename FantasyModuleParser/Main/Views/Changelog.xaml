<Window x:Class="FantasyModuleParser.Main.Views.Changelog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FantasyModuleParser.Main.Views"
        mc:Ignorable="d"
        Title="Updates to Fantasy Module Parser (Changelog)" Height="300" Width="560" ResizeMode="NoResize" Icon="/Resources/es2_logo_Jsv_icon.ico">
    <Grid Background="#FFE5E5E5">
        <ScrollViewer Margin="5,0,5,0" Grid.Column="3" Grid.Row="1">
            <ItemsControl Name="ChangeLogItemsControl" ItemsSource="{Binding ChangelogList}">
                <ItemsControl.ItemTemplate>
                    <DataTemplate>
                        <TextBlock Text="{Binding ver}" />
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ItemsControl>
        </ScrollViewer>
    </Grid>
</Window>
